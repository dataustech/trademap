{
    "extends": "airbnb-base",
    "rules": {
        "max-len": 1,
        "no-console": 0,
        "comma-dangle": 0,
        "no-plusplus": 0,
        "no-underscore-dangle": 1,
        "no-restricted-globals": 0,
        "no-mixed-operators": [
          "error",
          { "allowSamePrecedence": true }
        ]
    }
}
