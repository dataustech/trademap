{
  "name": "trademap",
  "version": "1.1.0",
  "description": "Trade map visualization",
  "scripts": {
    "build:nisra": "npm run clean:nisra && npm run preprocess:nisra && webpack --progress --display-error-details --config webpack.prod.js",
    "build:comtrade": "npm run clean:nisra && npm run preprocess:comtrade &&  webpack  --progress --display-error-details --config webpack.prod.js",
    "clean:nisra": "rimraf dist/nisra*",
    "clean:comtrade": "rimraf dist/comtrade*",
    "preprocess:nisra": "set project=nisra && node preprocess.nisra.js",
    "preprocess:comtrade": "set project=comtrade && node preprocess.comtrade.js",
    "start:nisra": "npm run clean:nisra && npm run preprocess:nisra && webpack-dev-server --config webpack.dev.js",
    "start:comtrade": "npm run clean:comtrade && npm run preprocess:comtrade && webpack-dev-server --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjs2020/trademap.git"
  },
  "author": "Francesco Merletti (me@fm.to.it)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mjs2020/trademap/issues"
  },
  "homepage": "https://github.com/mjs2020/trademap#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "autoprefixer": "^8.0.0",
    "babel-loader": "^8.0.0-beta.0",
    "css-loader": "^0.28.10",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.7",
    "html-loader": "^0.5.5",
    "json-loader": "^0.5.7",
    "mkdirp": "^0.5.1",
    "modernizr-loader": "^1.0.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.0",
    "precss": "^3.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.11.0",
    "webpack-clear-console": "^1.0.3",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "zip-webpack-plugin": "^2.1.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "crossfilter": "^1.3.12",
    "d3": "^4.13.0",
    "d3-geo": "^1.9.1",
    "d3-geo-projection": "^2.3.2",
    "d3-tip": "^0.7.1",
    "file-saver": "^1.3.3",
    "intro.js": "git+https://github.com/usablica/intro.js.git#5fe318653267b9738a2a185402b566cc90aa37ac",
    "jquery": "^3.3.1",
    "modernizr": "^3.5.0",
    "popper.js": "^1.12.9",
    "select2": "^4.0.6-rc.1",
    "select2-bootstrap-theme": "0.1.0-beta.10",
    "topojson-client": "^3.0.0"
  }
}
